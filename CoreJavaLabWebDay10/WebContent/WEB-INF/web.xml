  <welcome-file-list>
    <welcome-file>content.html</welcome-file>
  </welcome-file-list>
  <listener>
    <listener-class>day10.module2.DBSetupListener</listener-class>
  </listener>
  <servlet>
    <description></description>
    <display-name>ListEmployees</display-name>
    <servlet-name>ListEmployees</servlet-name>
